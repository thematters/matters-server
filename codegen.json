{
  "overwrite": true,
  "schema": "schema.graphql",
  "generates": {
    "src/definitions/schema.d.ts": {
      "plugins": [
        "typescript",
        "typescript-resolvers",
        { "add": { "content": "import { GlobalId } from './nominal.js'" } }
      ],
      "config": {
        "useIndexSignature": true,
        "typesPrefix": "GQL",
        "namingConvention": {
          "enumValues": "keep"
        },
        "inputMaybeValue": "T | undefined",
        "mapperTypeSuffix": "Model",
        "mappers": {
          "User": "./user.js#User",
          "UserInfo": "./user.js#User",
          "UserStatus": "./user.js#User",
          "UserActivity": "./user.js#User",
          "UserSettings": "./user.js#User",
          "UserAnalytics": "./user.js#User",
          "UserOSS": "./user.js#User",
          "Recommendation": "./user.js#User",
          "Following": "./user.js#User",
          "Liker": "./user.js#User",
          "Wallet": "./user.js#User",
          "CryptoWallet": "./user.js#Wallet",
          "Tag": "./tag.js#Tag",
          "TagOSS": "./tag.js#Tag",
          "Collection": "./collection.js#Collection",
          "Comment": "./comment.js#Comment",
          "Article": "./article.js#Article",
          "ArticleAccess": "./article.js#Article",
          "ArticleOSS": "./article.js#Article",
          "ArticleVersion": "./article.js#ArticleVersion",
          "ArticleContents": "./article.js#ArticleVersion",
          "Draft": "./draft.js#Draft",
          "DraftAccess": "./draft.js#Draft",
          "Circle": "./circle.js#Circle",
          "Invites": "./circle.js#Circle",
          "Invitation": "./circle.js#CircleInvitation",
          "CircleAnalytics": "./circle.js#Circle",
          "CircleIncomeAnalytics": "./circle.js#Circle",
          "CircleSubscriberAnalytics": "./circle.js#Circle",
          "CircleFollowerAnalytics": "./circle.js#Circle",
          "CircleContentAnalytics": "./circle.js#Circle",
          "Member": "./circle.js#CircleMember",
          "Price": "./index.js#CirclePrice",
          "StripeAccount": "./payment.js#PayoutAccount",
          "Transaction": "./index.js#Transaction",
          "Asset": "./asset.js#Asset",
          "Notice": "./notification.js#NoticeItem",
          "UserNotice": "./notification.js#NoticeItem",
          "ArticleNotice": "./notification.js#NoticeItem",
          "ArticleArticleNotice": "./notification.js#NoticeItem",
          "ArticleTagNotice": "./notification.js#NoticeItem",
          "CollectionNotice": "./notification.js#NoticeItem",
          "MomentNotice": "./notification.js#NoticeItem",
          "TagNotice": "./notification.js#NoticeItem",
          "CommentNotice": "./notification.js#NoticeItem",
          "CommentCommentNotice": "./notification.js#NoticeItem",
          "CampaignArticleNotice": "./notification.js#NoticeItem",
          "TransactionNotice": "./notification.js#NoticeItem",
          "CircleNotice": "./notification.js#NoticeItem",
          "OfficialNotice": "./notification.js#NoticeItem",
          "OfficialAnnouncementNotice": "./notification.js#NoticeItem",
          "Appreciation": "./appreciation.js#Appreciation",
          "OAuthClient": "./user.js#OAuthClientDB",
          "Report": "./report.js#Report",
          "IcymiTopic": "./misc.js#MattersChoiceTopic",
          "Moment": "./moment.js#Moment",
          "Writing": "./index.js#Writing",
          "Campaign": "./campaign.js#Campaign",
          "CampaignOSS": "./campaign.js#Campaign",
          "WritingChallenge": "./campaign.js#Campaign",
          "CampaignStage": "./campaign.js#CampaignStage",
          "Channel": "./channel.js#Channel"
        },
        "scalars": {
          "ID": {
            "input": "GlobalId",
            "output": "GlobalId"
          }
        },
        "contextType": "./index.js#Context",
        "makeResolverTypeCallable": true,
        "onlyResolveTypeForInterfaces": true,
        "enumsAsTypes": true
      }
    }
  }
}
