{
  "overwrite": true,
  "schema": "schema.graphql",
  "generates": {
    "src/definitions/schema.d.ts": {
      "plugins": ["typescript", "typescript-resolvers"],
      "config": {
        "useIndexSignature": true,
        "typesPrefix": "GQL",
        "namingConvention": {
          "enumValues": "keep"
        },
        "inputMaybeValue": "T | undefined",
        "mapperTypeSuffix": "Model",
        "mappers": {
          "User": "./user#User",
          "UserInfo": "./user#User",
          "UserStatus": "./user#User",
          "UserActivity": "./user#User",
          "UserSettings": "./user#User",
          "UserAnalytics": "./user#User",
          "UserOSS": "./user#User",
          "Recommendation": "./user#User",
          "Following": "./user#User",
          "Liker": "./user#User",
          "Wallet": "./user#User",
          "CryptoWallet": "./user#Wallet",
          "Tag": "./tag#Tag",
          "TagOSS": "./tag#Tag",
          "Collection": "./collection#Collection",
          "Comment": "./comment#Comment",
          "Article": "./article#Article",
          "ArticleAccess": "./article#Article",
          "ArticleOSS": "./article#Article",
          "ArticleVersion": "./article#ArticleVersion",
          "ArticleContents": "./article#ArticleVersion",
          "Draft": "./draft#Draft",
          "DraftAccess": "./draft#Draft",
          "Circle": "./circle#Circle",
          "Invites": "./circle#Circle",
          "Invitation": "./circle#CircleInvitation",
          "CircleAnalytics": "./circle#Circle",
          "CircleIncomeAnalytics": "./circle#Circle",
          "CircleSubscriberAnalytics": "./circle#Circle",
          "CircleFollowerAnalytics": "./circle#Circle",
          "CircleContentAnalytics": "./circle#Circle",
          "Member": "./circle#CircleMember",
          "Price": "./index#CirclePrice",
          "StripeAccount": "./payment#PayoutAccount",
          "Transaction": "./index#Transaction",
          "Asset": "./asset#Asset",
          "Notice": "./notification#NoticeItem",
          "UserNotice": "./notification#NoticeItem",
          "ArticleNotice": "./notification#NoticeItem",
          "ArticleArticleNotice": "./notification#NoticeItem",
          "ArticleTagNotice": "./notification#NoticeItem",
          "TagNotice": "./notification#NoticeItem",
          "CommentNotice": "./notification#NoticeItem",
          "CommentCommentNotice": "./notification#NoticeItem",
          "TransactionNotice": "./notification#NoticeItem",
          "CircleNotice": "./notification#NoticeItem",
          "OfficialNotice": "./notification#NoticeItem",
          "OfficialAnnouncementNotice": "./notification#NoticeItem",
          "Appreciation": "./appreciation#Appreciation",
          "OAuthClient": "./user#OAuthClientDB",
          "Report": "./report#Report",
          "IcymiTopic": "./misc#MattersChoiceTopic"
        },
        "contextType": "./index#Context",
        "makeResolverTypeCallable": true,
        "onlyResolveTypeForInterfaces": true,
        "enumsAsTypes": true
      }
    }
  }
}
