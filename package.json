{
  "name": "matters-server",
  "version": "2.6.0",
  "description": "Matters Server",
  "main": "build/index.js",
  "engines": {
    "node": ">=10.15",
    "npm": ">=6.5.0"
  },
  "scripts": {
    "db:rollback": "knex-migrate rollback",
    "db:migrate": "knex-migrate up",
    "db:migration:make": "knex-migrate --stub db/stub/migration.stub generate",
    "db:seed": "knex seed:run",
    "db:seed:make": "knex seed:make",
    "clean": "rimraf build",
    "build:email": "mjml src/connectors/mail/templates/*.mjml -o src/connectors/mail/templates/build/",
    "build": "tsc -p .",
    "watch:build": "tsc -p . --watch",
    "watch:server": "nodemon build/index.js",
    "start": "node build/index.js",
    "start:dev": "npm-run-all clean build --parallel watch:build watch:server --print-label",
    "start:dev:email": "mjml -w src/connectors/mail/templates/*.mjml -o src/connectors/mail/templates/build/",
    "test:connectors": "MATTERS_ENV=test jest src/connectors --forceExit",
    "test:types": "MATTERS_ENV=test jest src/types --forceExit",
    "test": "npm run test:connectors && npm run test:types && codecov",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix",
    "format": "prettier --write \"{,!(node_modules|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "format:check": "npm run format -- --list-different",
    "gen:schema": "ts-node --files src/common/utils/exportSchema.ts",
    "gen:types": "ts-node --files src/common/utils/generateTypes.ts",
    "gen": "npm run gen:schema && npm run gen:types",
    "schema:push": "apollo service:push --key=$MATTERS_APOLLO_API_KEY --localSchemaFile=schema.graphql"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@elastic/elasticsearch": "^7.5.0",
    "@matters/slugify": "^0.7.3",
    "@okgrow/graphql-scalars": "^0.4.6",
    "@sendgrid/mail": "^6.4.0",
    "@sentry/node": "^5.11.1",
    "apollo-server-cache-redis": "^1.1.5",
    "apollo-server-express": "^2.9.12",
    "apollo-server-plugin-response-cache": "^0.3.11",
    "aws-sdk": "^2.609.0",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.7",
    "bodybuilder": "^2.2.20",
    "bull": "^3.12.1",
    "cheerio": "^1.0.0-rc.2",
    "cookie": "^0.4.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "firebase-admin": "^8.9.1",
    "graphql": "^14.5.8",
    "graphql-cost-analysis": "^1.0.3",
    "graphql-custom-types": "^1.5.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^4.0.2",
    "graphql-redis-subscriptions": "^2.1.2",
    "graphql-relay": "^0.6.0",
    "graphql-schema-typescript": "^1.3.2",
    "graphql-tools": "^4.0.6",
    "graphql-type-json": "^0.3.1",
    "graphql-upload": "^9.0.0",
    "helmet": "^3.21.2",
    "image-size": "^0.8.3",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "ipfs-http-client": "^28.1.0",
    "js-base64": "^2.5.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.26",
    "module-alias": "^2.2.2",
    "nanoid": "^2.1.9",
    "newrelic": "^6.2.0",
    "oauth2-server": "^3.0.1",
    "objection": "^2.1.2",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0",
    "pg": "^7.17.1",
    "psl": "^1.7.0",
    "qs": "^6.9.1",
    "request-ip": "^2.1.3",
    "sharp": "^0.24.0",
    "typescript": "^3.7.5",
    "uuid": "^3.4.0",
    "validator": "^12.1.0",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.12.0",
    "@types/cheerio": "^0.22.15",
    "@types/cookie": "^0.3.3",
    "@types/dotenv": "^8.2.0",
    "@types/express-session": "^1.15.16",
    "@types/graphql": "^14.5.0",
    "@types/graphql-bigint": "^1.0.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/graphql-iso-date": "^3.3.3",
    "@types/graphql-relay": "^0.4.11",
    "@types/graphql-type-json": "^0.3.2",
    "@types/helmet": "0.0.45",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-gifsicle": "^5.2.0",
    "@types/imagemin-jpegtran": "^5.0.0",
    "@types/imagemin-svgo": "^7.0.0",
    "@types/ioredis": "^4.14.4",
    "@types/jest": "^24.9.0",
    "@types/js-base64": "^2.3.1",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.149",
    "@types/mime-types": "^2.1.0",
    "@types/nanoid": "^2.1.0",
    "@types/newrelic": "^5.11.2",
    "@types/node-fetch": "^2.5.4",
    "@types/oauth2-server": "^3.0.12",
    "@types/passport": "^1.0.2",
    "@types/passport-oauth2": "^1.4.8",
    "@types/psl": "^1.1.0",
    "@types/qs": "^6.9.0",
    "@types/request-ip": "0.0.34",
    "@types/sharp": "^0.24.0",
    "@types/uuid": "^3.4.6",
    "@types/validator": "^12.0.1",
    "@types/winston": "^2.4.4",
    "@types/ws": "^7.2.0",
    "apollo-server-testing": "^2.9.12",
    "chokidar": "^3.3.1",
    "chokidar-cli": "^2.1.0",
    "codecov": "^3.6.1",
    "husky": "^4.2.0",
    "jest": "^24.9.0",
    "knex-migrate": "^1.7.4",
    "mjml": "^4.5.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "_moduleAliases": {
    "common": "./build/common",
    "connectors": "./build/connectors",
    "definitions": "./build/definitions",
    "middlewares": "./build/middlewares",
    "mutations": "./build/mutations",
    "queries": "./build/queries",
    "types": "./build/types"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run gen && npm run format && git add schema.graphql src/definitions/schema.d.ts"
    }
  },
  "graphql-inspector": {
    "diff": true,
    "schema": {
      "ref": "develop",
      "path": "schema.graphql"
    }
  }
}
