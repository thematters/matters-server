{
  "name": "matters-server",
  "version": "0.0.1",
  "description": "Matters Server",
  "main": "build/index.js",
  "engines": {
    "node": ">=10.14"
  },
  "scripts": {
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback ",
    "db:seed": "knex seed:run",
    "clean": "rimraf build",
    "build": "tsc -p .",
    "watch:build": "tsc -p . --watch",
    "watch:server": "nodemon build/index.js",
    "postinstall": "npm run build",
    "start": "node build/index.js",
    "start:dev": "npm-run-all clean build --parallel watch:build watch:server --print-label",
    "test": "jest",
    "tslint:dev": "tslint --force -c tslint.json 'src/**/*.ts{,x}' -e '**/node_modules/**/*'",
    "tslint:watch:dev": "chokidar 'src/**/*.ts{,x}' -c 'npm run tslint:dev'",
    "tslint:production": "node_modules/.bin/tslint -t verbose -c tslint.json 'src/**/*.ts{,x}' -e '**/node_modules/**/*'",
    "format": "prettier --single-quote --write \"{,!(node_modules|build)/**/}*.{js,jsx,ts,tsx,json}\"",
    "format:check": "npm run format -- --list-different",
    "exportSchema": "npm run build && node build/common/utils/exportSchema.js"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "apollo-server": "^2.2.0",
    "bcrypt": "^3.0.2",
    "cheerio": "^1.0.0-rc.2",
    "dataloader": "^1.4.0",
    "graphql": "^14.0.2",
    "graphql-custom-types": "^1.5.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-redis-subscriptions": "^2.0.0",
    "graphql-tools": "^4.0.3",
    "graphql-type-json": "^0.2.1",
    "graphql-upload": "^8.0.2",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.15.2",
    "lodash": "^4.17.11",
    "module-alias": "^2.1.0",
    "objection": "^1.4.0",
    "pg": "^7.6.1",
    "typescript": "^3.1.6",
    "uuid": "^3.1.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cheerio": "^0.22.6",
    "@types/graphql": "^14.0.3",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/graphql-type-json": "^0.1.3",
    "@types/ioredis": "^4.0.4",
    "@types/jest": "^23.3.10",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/knex": "^0.15.1",
    "@types/lodash": "^4.14.117",
    "@types/uuid": "^3.4.3",
    "@types/validator": "^9.4.3",
    "@types/ws": "^6.0.1",
    "chokidar": "^2.0.3",
    "chokidar-cli": "^1.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "prettier": "1.14.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0"
  },
  "optionalDependencies": {
    "bufferutil": "^3.0.2",
    "utf-8-validate": "^5.0.0"
  },
  "_moduleAliases": {
    "common": "./build/common",
    "connectors": "./build/connectors",
    "definitions": "./build/definitions",
    "mutations": "./build/mutations",
    "queries": "./build/queries",
    "types": "./build/types"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
