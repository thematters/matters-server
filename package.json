{
  "name": "matters-server",
  "version": "2.1.0",
  "description": "Matters Server",
  "main": "build/index.js",
  "engines": {
    "node": ">=10.15",
    "npm": ">=6.5.0"
  },
  "scripts": {
    "db:rollback": "knex-migrate rollback",
    "db:migrate": "knex-migrate up",
    "db:migration:make": "knex-migrate --stub db/stub/migration.stub generate",
    "db:seed": "knex seed:run",
    "db:seed:make": "knex seed:make",
    "clean": "rimraf build",
    "build:email": "mjml src/connectors/mail/templates/*.mjml -o src/connectors/mail/templates/build/",
    "build": "tsc -p .",
    "watch:build": "tsc -p . --watch",
    "watch:server": "nodemon build/index.js",
    "start": "node build/index.js",
    "start:dev": "npm-run-all clean build --parallel watch:build watch:server --print-label",
    "start:dev:email": "mjml -w src/connectors/mail/templates/*.mjml -o src/connectors/mail/templates/build/",
    "test:connectors": "MATTERS_ENV=test jest src/connectors --forceExit",
    "test:types": "MATTERS_ENV=test jest src/types --forceExit",
    "test": "npm run test:connectors && npm run test:types && codecov",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix",
    "format": "prettier --write \"{,!(node_modules|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "format:check": "npm run format -- --list-different",
    "gen:schema": "ts-node --files src/common/utils/exportSchema.ts",
    "gen:types": "ts-node --files src/common/utils/generateTypes.ts",
    "gen": "npm run gen:schema && npm run gen:types",
    "schema:push": "apollo service:push --key=$MATTERS_APOLLO_API_KEY --localSchemaFile=schema.graphql"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@elastic/elasticsearch": "^7.3",
    "@matters/slugify": "^0.7.3",
    "@okgrow/graphql-scalars": "^0.4.5",
    "@sendgrid/mail": "^6.4.0",
    "@sentry/node": "^5.7.1",
    "apollo-server-cache-redis": "^1.1.1",
    "apollo-server-express": "^2.7.0",
    "apollo-server-plugin-response-cache": "^0.3.1",
    "aws-sdk": "^2.494.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bodybuilder": "^2.2.17",
    "bull": "^3.10.0",
    "cheerio": "^1.0.0-rc.2",
    "cookie": "^0.4.0",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "graphql-cost-analysis": "^1.0.3",
    "graphql-custom-types": "^1.5.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^4.0.1",
    "graphql-redis-subscriptions": "^2.1.0",
    "graphql-relay": "^0.6.0",
    "graphql-schema-typescript": "^1.2.9",
    "graphql-tools": "^4.0.5",
    "graphql-type-json": "^0.3.0",
    "graphql-upload": "^8.0.7",
    "helmet": "^3.21.2",
    "image-size": "^0.7.4",
    "imagemin": "^7.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "ipfs-http-client": "^28.1.0",
    "jpush-async": "^4.0.2",
    "js-base64": "^2.5.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "lodash": "^4.17.14",
    "mime-types": "^2.1.24",
    "module-alias": "^2.2.0",
    "nanoid": "^2.1.0",
    "newrelic": "^5.10.0",
    "oauth2-server": "^3.0.1",
    "objection": "^1.6.9",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.5.0",
    "pg": "^7.11.0",
    "psl": "^1.2.0",
    "request-ip": "^2.1.3",
    "sharp": "^0.22.1",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2",
    "validator": "^10.11.0",
    "winston": "^3.2.1",
    "xss": "^1.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.10.0",
    "@types/cheerio": "^0.22.12",
    "@types/cookie": "^0.3.3",
    "@types/dotenv": "^6.1.1",
    "@types/graphql": "^14.2.2",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/graphql-relay": "^0.4.9",
    "@types/graphql-type-json": "^0.3.0",
    "@types/helmet": "0.0.43",
    "@types/image-size": "^0.7.0",
    "@types/imagemin": "^6.0.0",
    "@types/imagemin-gifsicle": "^5.2.0",
    "@types/imagemin-jpegtran": "^5.0.0",
    "@types/imagemin-svgo": "^7.0.0",
    "@types/ioredis": "^4.0.13",
    "@types/jest": "^24.0.15",
    "@types/js-base64": "^2.3.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.136",
    "@types/mime-types": "^2.1.0",
    "@types/nanoid": "^2.0.0",
    "@types/newrelic": "^4.11.0",
    "@types/oauth2-server": "^3.0.10",
    "@types/passport": "^1.0.0",
    "@types/passport-oauth2": "^1.4.8",
    "@types/psl": "^1.1.0",
    "@types/request-ip": "0.0.34",
    "@types/sharp": "^0.22.2",
    "@types/uuid": "^3.4.5",
    "@types/validator": "^10.11.1",
    "@types/winston": "^2.4.4",
    "@types/ws": "^6.0.1",
    "apollo": "^2.16.0",
    "apollo-server-testing": "^2.7.0",
    "chokidar": "^3.0.2",
    "chokidar-cli": "^2.0.0",
    "codecov": "^3.5.0",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "knex-migrate": "^1.7.2",
    "mjml": "^4.4.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "_moduleAliases": {
    "common": "./build/common",
    "connectors": "./build/connectors",
    "definitions": "./build/definitions",
    "middlewares": "./build/middlewares",
    "mutations": "./build/mutations",
    "queries": "./build/queries",
    "types": "./build/types"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run gen && npm run format && git add schema.graphql src/definitions/schema.d.ts"
    }
  },
  "graphql-inspector": {
    "diff": true,
    "schema": {
      "ref": "develop",
      "path": "schema.graphql"
    }
  }
}
