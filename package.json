{
  "name": "matters-server",
  "version": "3.28.0",
  "description": "Matters Server",
  "author": "Matters <hi@matters.news>",
  "main": "build/index.js",
  "engines": {
    "node": ">=16"
  },
  "license": "Apache-2.0",
  "scripts": {
    "db:rollback": "knex-migrate rollback",
    "db:migrate": "knex-migrate up",
    "db:migration:make": "knex-migrate --stub db/stub/migration.stub generate",
    "db:seed": "knex seed:run",
    "db:seed:make": "knex seed:make",
    "clean": "rimraf build",
    "build": "tsc -p .",
    "watch:build": "tsc -p . --watch",
    "watch:server": "nodemon build/index.js",
    "start": "node build/index.js",
    "start:dev": "npm-run-all clean build --parallel watch:build watch:server --print-label",
    "test:connectors": "MATTERS_ENV=test jest src/connectors --forceExit",
    "test:utils": "MATTERS_ENV=test jest src/common/utils --forceExit",
    "test:types": "MATTERS_ENV=test jest src/types --forceExit",
    "test": "npm run test:utils && npm run test:connectors && npm run test:types && codecov",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix",
    "format": "prettier --write \"{,!(node_modules|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "format:check": "npm run format -- --list-different",
    "gen:schema": "ts-node --files src/common/utils/exportSchema.ts",
    "gen:types": "ts-node --files src/common/utils/generateTypes.ts",
    "gen": "npm run gen:schema && npm run gen:types",
    "schema:push:dev": "rover graph publish matters-stage@current --schema ./schema.graphql",
    "schema:push:prod": "rover graph publish matters-production@current --schema ./schema.graphql",
    "search:init": "ts-node --files src/common/utils/initSearchIndices.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.13.0",
    "@google-cloud/translate": "^6.2.6",
    "@graphql-tools/schema": "^7.1.5",
    "@graphql-tools/utils": "^7.10.0",
    "@matters/apollo-response-cache": "^1.4.0-rc.0",
    "@matters/matters-html-formatter": "0.0.7",
    "@matters/passport-likecoin": "^1.0.0",
    "@matters/slugify": "^0.7.3",
    "@sendgrid/mail": "^7.4.5",
    "@sentry/node": "^5.20.0",
    "@slack/web-api": "^6.5.1",
    "apollo-cache-control": "^0.11.1",
    "apollo-datasource": "^3.0.2",
    "apollo-datasource-rest": "^3.3.0",
    "apollo-server-cache-redis": "^3.0.2",
    "apollo-server-core": "^3.0.2",
    "apollo-server-express": "^3.0.2",
    "aws-sdk": "^2.954.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bodybuilder": "^2.4.0",
    "bull": "^3.26.0",
    "cheerio": "^1.0.0-rc.10",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^10.0.0",
    "eth-sig-util": "^3.0.1",
    "express": "4.17.1",
    "fastest-levenshtein": "^1.0.12",
    "firebase-admin": "^9.11.0",
    "get-stream": "^6.0.1",
    "graphql": "^15.5.1",
    "graphql-constraint-directive": "^2.1.1",
    "graphql-cost-analysis": "^1.0.3",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^6.0.10",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-relay": "^0.8.0",
    "graphql-scalars": "^1.10.0",
    "graphql-schema-typescript": "^1.5.2",
    "graphql-upload": "^12.0.0",
    "helmet": "^4.6.0",
    "ipfs-http-client": "^50.1.2",
    "js-base64": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.31",
    "module-alias": "^2.2.2",
    "nanoid": "^3.1.23",
    "number-precision": "^1.5.0",
    "oauth2-server": "^3.1.1",
    "objection": "^2.2.16",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.6.0",
    "pg": "^8.6.0",
    "psl": "^1.8.0",
    "qs": "^6.10.1",
    "request-ip": "^2.1.3",
    "stripe": "^8.165.0",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2",
    "validator": "^13.6.0",
    "web3": "^1.6.0",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0",
    "xss": "^1.0.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.2",
    "@types/cookie": "^0.4.1",
    "@types/dotenv": "^8.2.0",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/graphql-relay": "^0.6.0",
    "@types/graphql-upload": "^8.0.6",
    "@types/ioredis": "^4.26.6",
    "@types/jest": "^26.0.24",
    "@types/js-base64": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.171",
    "@types/mime-types": "^2.1.0",
    "@types/nanoid": "^2.1.0",
    "@types/node-fetch": "^2.5.12",
    "@types/oauth2-server": "^3.0.13",
    "@types/passport": "^1.0.7",
    "@types/passport-oauth2": "^1.4.11",
    "@types/psl": "^1.1.0",
    "@types/qs": "^6.9.7",
    "@types/request-ip": "0.0.37",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.6.3",
    "@types/winston": "^2.4.4",
    "@types/ws": "^7.4.7",
    "chokidar": "^3.5.2",
    "chokidar-cli": "^2.1.0",
    "codecov": "^3.8.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "knex-migrate": "^1.7.4",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "utf-8-validate": "^5.0.5"
  },
  "_moduleAliases": {
    "common": "./build/common",
    "connectors": "./build/connectors",
    "definitions": "./build/definitions",
    "middlewares": "./build/middlewares",
    "mutations": "./build/mutations",
    "queries": "./build/queries",
    "types": "./build/types"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "graphql-inspector": {
    "diff": true,
    "schema": {
      "ref": "develop",
      "path": "schema.graphql"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
