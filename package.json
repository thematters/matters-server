{
  "name": "matters-server",
  "version": "5.9.3",
  "description": "Matters Server",
  "author": "Matters <hi@matters.news>",
  "type": "module",
  "main": "build/index.js",
  "engines": {
    "node": ">=18.18.0 <19.0.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "db:rollback": "knex migrate:rollback",
    "db:migrate": "knex migrate:latest",
    "db:migration:make": "knex --stub db/stub/migration.stub generate",
    "db:seed": "knex seed:run",
    "db:seed:make": "knex seed:make",
    "clean": "rimraf build",
    "build": "tsc -p .",
    "watch:build": "tsc -p . --watch",
    "watch:server": "nodemon build/index.js",
    "start": "node build/index.js",
    "start:dev": "npm-run-all clean build --parallel watch:build watch:server --print-label",
    "testcmd": "MATTERS_ENV=test node --experimental-vm-modules node_modules/.bin/jest",
    "test:connectors": "npm run testcmd -- build/connectors/ --forceExit",
    "test:utils": "npm run testcmd -- build/common/utils/",
    "test:routes": "npm run testcmd -- build/routes/ --forceExit",
    "test:types:1": "npm run testcmd -- build/types/__test__/1/ --forceExit",
    "test:types:2": "npm run testcmd -- build/types/__test__/2/ --forceExit",
    "test": "run-s build test:* test:types:* && codecov",
    "lint": "eslint . --ext .ts --quiet",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"{,!(node_modules|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "format:check": "npm run format -- --list-different",
    "gen:schema": "tsc -p . && node build/common/utils/exportSchema.js",
    "gen:types": "graphql-codegen-esm --config codegen.json",
    "gen": "npm run gen:schema && npm run gen:types",
    "schema:push:dev": "rover graph publish matters-stage@current --schema ./schema.graphql",
    "schema:push:prod": "rover graph publish matters-production@current --schema ./schema.graphql",
    "prepare": "husky"
  },
  "dependencies": {
    "@alchemy/aa-accounts": "^3.19.0",
    "@alchemy/aa-core": "^3.19.0",
    "@apollo/cache-control-types": "^1.0.2",
    "@apollo/datasource-rest": "^6.0.0",
    "@apollo/server": "^4.7.4",
    "@apollo/utils.keyvadapter": "^3.0.0",
    "@apollo/utils.keyvaluecache": "^3.0.0",
    "@google-cloud/translate": "^8.5.1",
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-tools/utils": "^10.0.0",
    "@keyv/redis": "^2.6.1",
    "@matters/apollo-response-cache": "git@github.com:thematters/apollo-response-cache.git#ee452e5006f0d989c8b2eeb9e8f52dbe30489fa3",
    "@matters/ipns-site-generator": "^0.1.7",
    "@matters/matters-editor": "^0.3.0-alpha.4",
    "@matters/passport-likecoin": "^1.0.0",
    "@matters/slugify": "^0.7.3",
    "@sendgrid/helpers": "^7.7.0",
    "@sendgrid/mail": "^8.1.4",
    "@sentry/node": "^7.52.1",
    "@slack/web-api": "^6.8.1",
    "apollo-server-plugin-query-complexity": "^4.0.0",
    "aws-sdk": "^2.1381.0",
    "axios": "^1.6.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "bodybuilder": "^2.5.0",
    "bull": "^4.10.4",
    "cheerio": "^1.0.0-rc.12",
    "cids": "^1.1.9",
    "cookie": "^1.0.2",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "fastest-levenshtein": "^1.0.16",
    "form-data": "^4.0.0",
    "get-stream": "^6.0.1",
    "graphql": "^16.8.1",
    "graphql-constraint-directive": "^5.1.1",
    "graphql-middleware": "^6.1.34",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-query-complexity": "^0.12.0",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.22.0",
    "graphql-upload": "^13.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.5.0",
    "ioredis-mock": "^8.9.0",
    "js-base64": "^3.7.5",
    "jsonwebtoken": "^9.0.0",
    "keyv": "^4.5.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "meilisearch": "^0.32.3",
    "mime-types": "^2.1.35",
    "nanoid": "^3.3.7",
    "node-fetch": "^2.6.11",
    "number-precision": "^1.6.0",
    "oauth-1.0a": "^2.2.6",
    "oauth2-server": "^3.1.1",
    "objection": "^3.0.2",
    "p-throttle": "^4.1.1",
    "passport": "^0.6.0",
    "passport-oauth2": "^1.7.0",
    "pg": "^8.11.0",
    "qs": "^6.11.2",
    "request-ip": "^3.3.0",
    "simplecc-wasm": "^1.1.0",
    "stripe": "^12.11.0",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "viem": "^2.21.52",
    "winston": "^3.8.2",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/graphql-upload": "^8.0.12",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.194",
    "@types/mime-types": "^2.1.1",
    "@types/nanoid": "^2.1.0",
    "@types/node-fetch": "^2.6.4",
    "@types/oauth2-server": "^3.0.13",
    "@types/passport": "^1.0.12",
    "@types/passport-oauth2": "^1.4.12",
    "@types/qs": "^6.9.7",
    "@types/request-ip": "0.0.37",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.12.2",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "codecov": "^3.8.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^47.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^6.0.3"
  },
  "imports": {
    "#root/*": {
      "dev-import": "./*",
      "default": "./*"
    },
    "#db/*": {
      "dev-import": "./db/*",
      "default": "./db/*"
    },
    "#common/*": {
      "dev-import": "./src/common/*",
      "default": "./build/common/*"
    },
    "#connectors/*": {
      "dev-import": "./src/connectors/*",
      "default": "./build/connectors/*"
    },
    "#definitions/*": {
      "dev-import": "./src/definitions/*",
      "default": "./build/definitions/*"
    },
    "#middlewares/*": {
      "dev-import": "./src/middlewares/*",
      "default": "./build/middlewares/*"
    },
    "#mutations/*": {
      "dev-import": "./src/mutations/*",
      "default": "./build/mutations/*"
    },
    "#queries/*": {
      "dev-import": "./src/queries/*",
      "default": "./build/queries/*"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "graphql-inspector": {
    "diff": true,
    "schema": {
      "ref": "develop",
      "path": "schema.graphql"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
