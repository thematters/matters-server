{
  "name": "matters-server",
  "version": "4.26.1",
  "description": "Matters Server",
  "author": "Matters <hi@matters.news>",
  "main": "build/index.js",
  "engines": {
    "node": ">=16.14 <17.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "db:rollback": "knex-migrate rollback",
    "db:migrate": "knex-migrate up",
    "db:migration:make": "knex-migrate --stub db/stub/migration.stub generate",
    "db:seed": "knex seed:run",
    "db:seed:make": "knex seed:make",
    "clean": "rimraf build",
    "build": "tsc -p .",
    "watch:build": "tsc -p . --watch",
    "watch:server": "nodemon build/index.js",
    "start": "node build/index.js",
    "start:dev": "npm-run-all clean build --parallel watch:build watch:server --print-label",
    "test:connectors": "MATTERS_ENV=test jest src/connectors --forceExit --maxWorkers=50%",
    "test:utils": "MATTERS_ENV=test jest src/common/utils --forceExit --maxWorkers=50%",
    "test:routes": "MATTERS_ENV=test jest src/routes/ --forceExit --maxWorkers=50%",
    "test:types": "MATTERS_ENV=test jest src/types/ --forceExit",
    "test:types:1": "MATTERS_ENV=test jest src/types/__test__/1/ --forceExit --maxWorkers=50%",
    "test:types:2": "MATTERS_ENV=test jest src/types/__test__/2/ --forceExit --maxWorkers=50%",
    "test": "npm run test:utils && npm run test:connectors && npm run test:routes && npm run test:types:1 && npm run test:types:2 && codecov",
    "lint": "eslint . --ext .ts --quiet",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"{,!(node_modules|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "format:check": "npm run format -- --list-different",
    "gen:schema": "tsc -p . && ts-node --files src/common/utils/exportSchema.ts",
    "gen:types": "graphql-codegen --config codegen.json",
    "gen": "npm run gen:schema && npm run gen:types",
    "schema:push:dev": "rover graph publish matters-stage@current --schema ./schema.graphql",
    "schema:push:prod": "rover graph publish matters-production@current --schema ./schema.graphql",
    "search:init": "ts-node --files src/common/utils/initSearchIndices.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/cache-control-types": "^1.0.2",
    "@apollo/datasource-rest": "^6.0.0",
    "@apollo/server": "^4.7.4",
    "@apollo/utils.keyvadapter": "^3.0.0",
    "@apollo/utils.keyvaluecache": "^3.0.0",
    "@ethersproject/abstract-provider": "^5.7.0",
    "@google-cloud/translate": "^7.2.1",
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-tools/utils": "^10.0.0",
    "@keyv/redis": "^2.6.1",
    "@matters/apollo-response-cache": "^2.0.0-alpha.0",
    "@matters/ipns-site-generator": "^0.1.3",
    "@matters/matters-editor": "^0.2.2",
    "@matters/passport-likecoin": "^1.0.0",
    "@matters/slugify": "^0.7.3",
    "@sendgrid/helpers": "^7.7.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^7.52.1",
    "@slack/web-api": "^6.8.1",
    "apollo-server-plugin-query-complexity": "^4.0.0",
    "aws-sdk": "^2.1381.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "bodybuilder": "^2.5.0",
    "bull": "^4.10.4",
    "cheerio": "^1.0.0-rc.12",
    "cids": "^1.1.9",
    "cookie": "^0.5.0",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.0.3",
    "eth-sig-util": "^3.0.1",
    "ethers": "^5.7.2",
    "express": "4.18.2",
    "fastest-levenshtein": "^1.0.16",
    "form-data": "^4.0.0",
    "get-stream": "^6.0.1",
    "graphql": "^16.6.0",
    "graphql-constraint-directive": "^5.1.1",
    "graphql-middleware": "^6.1.34",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-query-complexity": "^0.12.0",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.22.0",
    "graphql-upload": "^13.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "ipfs-http-client": "^56.0.3",
    "js-base64": "^3.7.5",
    "jsonwebtoken": "^9.0.0",
    "keyv": "^4.5.2",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "meilisearch": "^0.32.3",
    "mime-types": "^2.1.35",
    "module-alias": "^2.2.2",
    "nanoid": "^3.3.6",
    "node-fetch": "^2.6.11",
